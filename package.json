{
	"name": "openf1-sdk",
	"version": "1.0.0",
	"description": "an sdk for interacting with openf1 api",
	"scripts": {
		"test": "jest --env=node",
		"clean": "tsc --build --clean",
		"build": "tsc  --project tsconfig.json && cp readme.md dist/readme.md",
		"pack": "cd dist && npm pack",
		"publish": "cd dist && npm publish --access public"
	},
	"keywords": ["f1", "formula1", "openf1", "sdk"],
	"author": "elarswa",
	"license": "MIT",
	"dependencies": {
		"axios": "^1.8.3"
	},
	"devDependencies": {
		"@babel/core": "^7.26.10",
		"@babel/preset-env": "^7.26.9",
		"@babel/preset-typescript": "^7.26.0",
		"@types/jest": "^29.5.14",
		"jest": "^29.7.0",
		"ts-jest": "^29.2.6",
		"ts-node": "^10.9.2",
		"typescript": "^5.8.2"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/elarswa/openf1-sdk.git"
	},
	"bugs": {
		"url": "https://github.com/elarswa/openf1-sdk/issues"
	},
	"homepage": "https://github.com/elarswa/openf1-sdk#readme"
